   1  sets
   2      m "machines" /X1, X2/
   3      b "jellybean colors" /yellow, blue, green, orange, purple/
   4      m_b(m, b) "valid combinations of m and b"
   5  ;
   6   
   7  alias(b,bb);
   8   
   9  * To start, all combinations are valid
  10  m_b(m,b) = yes;
